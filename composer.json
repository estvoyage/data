{
	"name": "estvoyage/data",
	"type": "library",
	"description": "Interfaces and classes to manipulate data in an east-oriented manner",
	"keywords": [ "data", "East" ],
	"homepage": "http://github.com/estvoyage/data",
	"license": "BSD",
	"authors": [
		{
			"name": "Frédéric Hardy",
			"email": "frederic.hardy@mageekbox.net",
			"homepage": "http://blog.mageekbox.net"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"estvoyage/value": "@dev"
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/mageekguy/atoum.git"
		}
	],
	"require-dev": {
		"mageekguy/atoum": "@dev"
	},
	"autoload": {
		"psr-4": {
			"estvoyage\\data\\": "classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"estvoyage\\data\\tests\\units\\": "tests/units"
		}
	}
}
